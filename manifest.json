{
    "manifest_version": 3,
    "name": "Mi Extensi√≥n de Calendario",
    "version": "1.0",
    "description": "Muestra eventos del calendario de Google",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
      }
  }

  